{
  "name": "nprogress",
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "description": "Simple slim progress bars",
  "version": "0.2.0",
  "main": "nprogress.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/nprogress.git"
  },
  "scripts": {
    "test": "mocha"
  },
  "main": "nprogress.js",
  "license": "MIT",
  "devDependencies": {
    "chai": "3.2.0",
    "jquery": "2.1.4",
    "jsdom": "6.3.0",
    "mocha": "2.3.1",
    "mocha-jsdom": "1.0.0",
    "less": "2.5.1"
  },
  "dependencies": {},
  "jspm": {
    "format": "global",
    "shim": {
      "nprogress": {
        "deps": [
          "./nprogress.css!"
        ]
      }
    },
    "dependencies": {
      "css": "*"
    }
  },
  "spm": {
    "main": "nprogress.js",
    "output": [
      "nprogress.css"
    ],
    "ignore": [
      "support",
      "test",
      "vendor"
    ]
  }
}
