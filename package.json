{
  "name": "nprogress",
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "description": "Simple slim progress bars",
  "version": "0.2.1-6",
  "main": "nprogress.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/nprogress.git"
  },
  "scripts": {
    "build": "bash ./build.sh",
    "test": "mocha"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "3.5.0",
    "jquery": "3.1.1",
    "jsdom": "9.9.1",
    "less": "2.7.2",
    "less-plugin-autoprefix": "1.5.1",
    "mocha": "3.2.0",
    "mocha-jsdom": "1.1.0",
    "keys": "github:GerHobbelt/keyscss#master"
  },
  "dependencies": {},
  "jspm": {
    "format": "global",
    "shim": {
      "nprogress": {
        "deps": [
          "./nprogress.css!"
        ]
      }
    },
    "dependencies": {
      "css": "*"
    }
  },
  "spm": {
    "main": "nprogress.js",
    "output": [
      "nprogress.css"
    ],
    "ignore": [
      "support",
      "test",
      "vendor"
    ]
  }
}
